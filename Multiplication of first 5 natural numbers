//Multiplication of first 5 natural numbers
import java.util.*;
public class Main
{
	public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
System.out.println("Enter any number");
int mul=1;
for(int i=1;i<=n;i++){
mul=mul*i;    
}
System.out.println(mul);
	}
}
